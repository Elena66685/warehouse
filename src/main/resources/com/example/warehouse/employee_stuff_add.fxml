<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.warehouse.EmployeeStuffAddController">
   <children>
      <ComboBox fx:id="user" layoutX="36.0" layoutY="53.0" onAction="#SetEmployeeStuffAdd" prefHeight="26.0" prefWidth="241.0" promptText="Сотрудник" />
      <ComboBox fx:id="stuff" layoutX="300.0" layoutY="53.0" onAction="#SetStuffAdd" prefHeight="26.0" prefWidth="197.0" promptText="Вещи" />
      <ComboBox fx:id="status" layoutX="300.0" layoutY="245.0" onAction="#SetStatusAdd" prefWidth="150.0" promptText="Статус" />
      <TextField fx:id="count" layoutX="37.0" layoutY="139.0" prefHeight="26.0" prefWidth="78.0" promptText="Количество" />
      <TextField fx:id="minus" layoutX="301.0" layoutY="140.0" promptText="Взяли " />
      <TextField fx:id="plus" layoutX="37.0" layoutY="246.0" promptText="Вернули" />
      <Button fx:id="back" layoutX="36.0" layoutY="333.0" mnemonicParsing="false" onAction="#OpenEmployeeStuffWindow" text="Назад" />
      <Button fx:id="send" layoutX="399.0" layoutY="333.0" mnemonicParsing="false" onAction="#sendEmployeeStuffAdd" text="Отправить" />
   </children>
</AnchorPane>
